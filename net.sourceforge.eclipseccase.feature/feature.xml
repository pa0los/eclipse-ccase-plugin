<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.sourceforge.eclipseccase"
      label="%featureName"
      version="2.2.35.qualifier"
      provider-name="%providerName"
      image="eclipse-ccase.jpg">

   <description>
      %description
   </description>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://eclipse-ccase.sourceforge.net/update/"/>
