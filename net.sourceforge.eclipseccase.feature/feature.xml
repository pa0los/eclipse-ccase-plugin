   </url>

   <requires>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.team.core"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.team.ui"/>
      <import plugin="org.eclipse.core.runtime" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.ui" version="3.3.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.console"/>
   </requires>

   <plugin
         id="net.sourceforge.clearcase"
         download-size="1000"
         install-size="1800"
         version="2.2.35.qualifier"/>

   <plugin
         id="net.sourceforge.eclipseccase"
         download-size="400"
         install-size="800"
         version="2.2.35.qualifier"
